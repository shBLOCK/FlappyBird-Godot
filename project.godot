config_version=5

[application]

config/name="FlappyBird"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
boot_splash/show_image=false
config/icon="res://textures/ui/icon.png"

[audio]

buses/default_bus_layout="res://sounds/bus_layout.tres"

[autoload]

Globals="*res://globals.gd"

[display]

window/size/viewport_width=170
window/size/viewport_height=220
window/size/window_width_override=510
window/size/window_height_override=660
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[filesystem]

import/blender/enabled=false

[gui]

common/snap_controls_to_pixels=false

